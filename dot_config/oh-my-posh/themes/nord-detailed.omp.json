{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#4C566A",
          "foreground": "#E5E9F0",
          "leading_diamond": "\ue0b2",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a ",
            "wsl": "\uf17c "
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#88C0D0",
          "foreground": "#2E3440",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " \uf489 {{ .Name }}",
          "type": "shell"
        },
        {
          "background": "#81A1C1",
          "foreground": "#E5E9F0",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " \ue266 {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
          "type": "sysinfo"
        },
        {
          "background": "#B48EAD",
          "foreground": "#E5E9F0",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " \ued0d {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "properties": {
            "display_mode": "files"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "angular"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\u03b1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "aurelia"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uebd8 {{ .EnvironmentName }} ",
          "type": "az"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf104<#EBCB8B>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue311  cds {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "cds"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uE370 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "crystal"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf40a  cf {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "cf"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf40a {{if .Org }}{{ .Org }}{{ end }}{{ if .Space }}/{{ .Space }}{{ end }} ",
          "type": "cftarget"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue61e \ue61d  cmake {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "cmake"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "dart"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue77f  {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue28e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "flutter"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue627 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue61f {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "haskell"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "K {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "kotlin"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf308 {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} ",
          "type": "kubectl"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "lua"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "Nx {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "nx"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue769 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "perl"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue73d {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "php"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "R {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "r"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",

          "type": "ruby"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#B48EAD",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue755 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "swift"
        },
        {
          "background": "#A3BE8C",
          "foreground": "#2E3440",
          "leading_diamond": "\ue0b2",
          "properties": {
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#5E81AC",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " ",
          "trailing_diamond": "<transparent,#5E81AC>\ue0b2</>",
          "type": "text"
        },
        {
          "background": "#434C5E",
          "foreground": "#E5E9F0",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#EBCB8B",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "#E5E9F0",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " \u2665 {{ .CurrentDate | date .Format }} |",
          "type": "time"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " \uf292 ",
          "type": "root"
        },
        {
          "foreground": "#E5E9F0",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#EBCB8B",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2570\u2500 ",
          "type": "status"
        },
        {
          "foreground": "#B48EAD",
          "style": "powerline",
          "template": "{{if .Root}}#{{else}}${{end}} ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#EBCB8B",
    "template": "\ue285 "
  },
  "version": 3
}

